{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "@comments scripts": [
    "lint: run eslint on all files",
    "lint:prettier: list eslint rules conflicting with prettier",
    "prettier:check: check format on all files",
    "prettier:write: format all files",
    "test: run jest for all files on watch mode"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "yarn eslint-config-prettier src/main",
    "prettier:check": "yarn prettier --check .",
    "prettier:write": "yarn prettier --write .",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "babel-jest": "^29.4.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "prettier": "^2.8.4",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.2",
    "whatwg-fetch": "^3.6.2"
  }
}
